{
  "presets": ["env", "stage-2"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-builtin-extend",
      {
        "globals": ["Error", "Array"]
      }
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
